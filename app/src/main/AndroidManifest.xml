<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zxzhang.leethub">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.QuestionDetailActivity"
            android:label="@string/title_activity_question_detail"
            android:theme="@style/AppTheme" />
        <activity android:name=".activity.ArticleActivity" />
        <activity android:name=".activity.QuestionDifficultyActivity" />
        <activity android:name=".activity.TopicsActivity" />
        <activity android:name=".activity.QuestionTopicActivity"></activity>
    </application>

</manifest>